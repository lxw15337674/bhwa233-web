{
    "functions": {
        "pages/**/*": {
            "maxDuration": 60
        }
    },
    "headers": [
        {
            "source": "/processor/image/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                },
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                }
            ]
        },
        {
            "source": "/processor/image",
            "headers": [
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                },
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                }
            ]
        }
    ],
    "buildCommand": "pnpm build",
    "outputDirectory": ".next"
}